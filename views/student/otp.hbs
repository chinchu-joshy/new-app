<section>
    <div class="container">
        <div class="row">
            <div class="col">
             <div class="card student-card">
  <img src="data:image/jpeg;base64," alt="John" style="width:100%">
  <h1>John Doe</h1>
  <p class="title">CEO & Founder, Example</p>
  <p>Harvard University</p>
               <form class="background-form otp-form" action="/verify" method="post" >
               <div class="form-group otp-otp">
    <label class="heading" for="">Mobile</label>
    <input type="tel" class="form-control" name="mobile" value="+91">
  </div> 
  <div class="form-group otp-otp">
    {{#if val}}
    <label class="heading false"for="">Invalid OTP OR Phone number </label>
    {{else}}
    <label class="heading"for="">Enter Your verification code </label>
    {{/if}}
    <input type="text" class="form-control" name="code">
  </div>
  
  <button type="submit" class="btn btn-success mt-4">Verify</button>
  </form> 
  </div>
            </div>
        </div>
    </div>
</section>