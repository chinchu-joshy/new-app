<div class="container">
  <div class="col">
    <h2 style="color: white;" class="teacher-payhead ">ATTENDANCE LIST</h2>
  </div>
    <div class="col-md-12  tab-class ">

    
    <table class="table mt-5 ml-2 table-responsive-md">
      <thead class="table-design" >
    <tr >
      
      
      
        <th scope="col"class="student-details">Date</th>
        <th scope="col"class="student-details">Attendance</th>
       
       
       
    </tr>
  </thead>  
    <tbody>
   
        {{#each details}}
        <tr>
     
       <td class="student">{{this.Date}}</td>
      
       <td class="student">{{this.attendance}}</td>
    
   
     
       
      
    
    </tr>
   {{/each}}
    </tbody>
    </table>
    </div>
    
</div>