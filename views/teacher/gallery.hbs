<div class="container">
  <div class="row">
        <div class="col-sm-8 mt-3">
         

          <form action="/upload-image" method="post" enctype="multipart/form-data">
           <div class="form-group">
              <label for="example-input-file">Type something.... </label>
              <input type="text"   name="title"   class="form-control-file border" required
               >
            </div>
            <div class="form-group">
              <label for="example-input-file"> </label>
             
		<input type="button" id="btnCrop" value="Crop" />
		<input type="button" id="btnRestore" value="Restore" />

              <input type="file"   name="photos"   class="form-control-file border" onchange="readURL(this);" 
               id="fileInput">
               <div>
               <fieldset id="canvas">
                      <img id="blah" src="#" alt="your image" style="display:block;
                      max-width:100%;"/>
                      </fieldset>
            </div>
            
            </div>
            <div id="result"></div>
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
           <div class="progress">
    <div class="bar"></div >
    <div class="percent">0%</div >
</div>

<div id="status"></div>
     
        </div>
        
      </div>
      <hr />
      <div class="row">
        <div class="col-sm-12">
            {{#each picture}}
                 
                 <img id="{{this._id}}" src='/gallery/{{this._id}}.png' style="
                 width:200px;height:200px display:block max-width:100%" onclick="imageCrop('{{this._id}}')">  
                 
          {{/each}}
        </div>

      </div>  
</div>


 