<div class="container">
      <div class="row">
        <div class="col-sm-8 mt-3">
         

          <form action="/multiple-upload" method="post" enctype="multipart/form-data">
            <div class="form-group">
              <label for="example-input-file"> </label>
              <input type="file"   name="photos" multiple id="input-multi-files" class="form-control-file border"/
               onchange="fileChange(event)"   >
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
        </div>
      </div>
      <hr />
      <div class="row">
        <div class="col-sm-12">
          <div class="preview-images"></div>
        </div>
      </div>
    </div>
    <script>
      var queue = [];

function fileChange(event) {
    var files = event.target.files, i = 0, j = files.length, file, reader;

    reader = new FileReader();
    reader.onloadend = function () {
        queue.push{ reader.result };
    };

    for (i = 0; i < j; i += 1) {
        file = files[i];
        reader.readAsBinaryString(file);
    }
}
    </script>

    